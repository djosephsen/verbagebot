heroku config:set HAL_SLACK_CHANNEL_MODE=blacklist
heroku config:set HAL_SLACK_TEAM=dbgone
heroku config:set HAL_SLACK_TOKEN=ruzOQremDBOjDc3Lv6YtIF0w
heroku config:set PORT=80
